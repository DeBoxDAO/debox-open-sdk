{
	"name": "debox-open-sdk",
	"version": "0.0.0",
	"private": true,
	"author": "vadxq",
	"license": "MIT",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@types/eslint": "^8.37.0",
		"@types/node": "^18.16.3",
		"@types/prettier": "^2.7.2",
		"dprint": "^0.35.4",
		"eslint": "^8.39.0",
		"eslint-config-custom": "workspace:*",
		"husky": "^8.0.3",
		"prettier": "^2.8.8",
		"turbo": "^1.9.3",
		"typescript": "^5.0.4"
	},
	"scripts": {
		"nodejs": "pnpm --filter=./apps/node",
		"nodejs:dev": "turbo run dev --scope=@monorepo/node",
		"rust:dev": "cargo watch -x run -p rust -w ./apps/rust",
		"build": "turbo run build --parallel",
		"dev": "turbo run dev --parallel --no-cache",
		"format": "pnpm dprint fmt",
		"lint": "turbo run lint --parallel",
		"prepare": "husky install",
		"changeset": "changeset",
		"publish:release": "changeset publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DeBoxDAO/debox-open-sdk.git"
	},
	"keywords": [
		"monorepo",
		"rust",
		"typescript",
		"solid"
	],
	"bugs": {
		"url": "https://github.com/DeBoxDAO/debox-open-sdk/issues"
	},
	"homepage": "https://github.com/DeBoxDAO/debox-open-sdk#readme",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"engines": {
		"node": ">=16.15.0"
	},
	"packageManager": "pnpm@8.4.0"
}
