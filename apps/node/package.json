{
	"name": "@deboxdao/debox-open-sdk",
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"playground": "node --experimental-specifier-resolution=node ./playground/index.js",
		"lint": "eslint \"**/*.{js,jsx,ts,tsx}\""
	},
